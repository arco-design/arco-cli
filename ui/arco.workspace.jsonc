{
  "arco.aspect/workspace": {
    "name": "Arco Workspace UI",
    "components": {
      "extends": {
        "rootDir": "src",
        "entries": {
          "main": "index.ts",
          "style": "style/index.ts",
          "preview": "__docs__/index.mdx",
          "jsdoc": [
            "interface.ts"
          ]
        }
      },
